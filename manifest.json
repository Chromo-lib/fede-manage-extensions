{
  "name": "fede",
  "version": "1.1.5",
  "description": "Free and open source tool to manage and control your browser extensions and apps more easily.",
  "manifest_version": 2,
  "icons": {
    "512": "assets/icon512.png",
    "128": "assets/icon128.png",
    "64": "assets/icon64.png"
  },
  "permissions": [
    "tabs",
    "management"
  ],
  "browser_action": {
    "default_title": "FEDE",
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "assets/*", "src/*"
 ]
}